{
  "name": "sklib",
  "version": "1.0.0",
  "description": "A utility to build, publish sketch libraries",
  "main": "lib/index.js",
  "preferGlobal": true,
  "bin": {
    "sklib": "lib/index.js"
  },
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "lib",
    "check.js"
  ],
  "scripts": {
    "start": "../../node_modules/.bin/babel-node src/index.js",
    "prebuild": "../../node_modules/.bin/rimraf lib",
    "build": "../../node_modules/.bin/babel src -d lib -D && chmod +x ./lib/index.js",
    "watch": "npm run build -- --watch",
    "prepare": "npm run -s build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.7.5",
    "chalk": "^3.0.0",
    "cross-spawn-promise": "^0.10.1",
    "fs.promised": "^3.0.0",
    "gh-pages": "^2.1.1",
    "gittar": "^0.1.1",
    "globby": "^10.0.1",
    "inquirer": "^7.0.0",
    "is-ci": "^2.0.0",
    "jszip": "^3.7.0",
    "keychain": "^1.3.0",
    "log-symbols": "^3.0.0",
    "open": "^7.0.0",
    "ora": "^4.0.3",
    "parse-author": "^2.0.0",
    "request": "^2.88.0",
    "update-notifier": "^3.0.1",
    "which": "^2.0.2",
    "xml2js": "^0.4.22",
    "yargs": "^15.0.2"
  },
  "targets": {
    "node": "8"
  }
}
